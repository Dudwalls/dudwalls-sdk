{
  "name": "dudwalls",
  "version": "1.0.0",
  "description": "Official SDK for Dudwalls NoSQL Database - A modern, self-hosted alternative to MongoDB and Firebase",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example:node": "node examples/node-example.js",
    "example:react": "cd examples/react-example && npm start",
    "example:vue": "cd examples/vue-example && npm run serve",
    "example:angular": "cd examples/angular-example && ng serve",
    "example:python": "python examples/python-example.py",
    "example:php": "php examples/php-example.php",
    "example:java": "cd examples/java-example && mvn exec:java",
    "example:go": "cd examples/go-example && go run main.go"
  },
  "keywords": [
    "nosql",
    "database",
    "mongodb",
    "firebase",
    "json",
    "document-store",
    "rest-api",
    "self-hosted",
    "dudwalls",
    "typescript",
    "javascript",
    "sdk"
  ],
  "author": "Dudwalls Team <support@dudwalls.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dudwalls/dudwalls-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/dudwalls/dudwalls-sdk/issues"
  },
  "homepage": "https://dudwalls.me",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "lib/**/*",
    "examples/**/*",
    "docs/**/*",
    "README.md",
    "LICENSE"
  ]
}
